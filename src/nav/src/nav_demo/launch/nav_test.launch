<launch>
<!--集成导航相关的launch文件-->
    <!-- 设置地图的配置文件 -->
    <arg name="map" default="new_map.yaml" />
    <!-- 运行地图服务器，并且加载设置的地图-->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find nav_demo)/maps/$(arg map)"/>
    <!-- 启动AMCL节点 -->
    <include file="$(find nav_demo)/launch/amcl_indoor.launch" />

    <!-- 运行move_base节点 -->
    <include file="$(find nav_demo)/launch/move_base.launch" />
    <!--可视化-->
    <node name="rviz"  pkg="rviz"  type="rviz"  args="-d $(find nav_demo)/rviz/navigation.rviz" />
</launch>
